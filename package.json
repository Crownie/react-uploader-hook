    "@types/node": "^13.13.2",
    "husky": "^4.2.5",
    "jest": "24.9.0",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-jest": "24.3.0",
    "typescript": "^3.8.3"
